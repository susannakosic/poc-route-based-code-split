{
  "homepage": "https://susannakosic.github.io/poc-route-based-code-split",
  "name": "react-ssr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:github": "tsc && vite build --base /poc-route-based-code-split/",
    "preview": "vite preview",
    "analyze": "tsc && vite build --mode analyze && mv analysis public",
    "predeploy": "npm run build:github",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "contentful": "^9.2.17",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "gh-pages": "^5.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
